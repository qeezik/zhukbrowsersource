function applyTheme(theme) {
    // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ "–í —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–µ!" –¥–ª—è —Å–≤–µ—Ç–ª–æ–π –∏ —Å–∏—Å—Ç–µ–º–Ω–æ–π —Ç–µ–º
    if (theme === 'light' || theme === 'system') {
        showNotification('–í —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–µ!', 'fa-tools');
        
        // –ü–µ—Ä–µ–∫–ª—é—á–∞–µ–º –æ–±—Ä–∞—Ç–Ω–æ –Ω–∞ —Ç–µ–º–Ω—É—é —Ç–µ–º—É
        const darkRadio = document.querySelector('input[name="themeMode"][value="dark"]');
        if (darkRadio) {
            darkRadio.checked = true;
        }
        
        return;
    }
    
    // –£–¥–∞–ª—è–µ–º —Å—Ç–∞—Ä—ã–π –æ–±—Ä–∞–±–æ—Ç—á–∏–∫, –µ—Å–ª–∏ –±—ã–ª
    if (systemThemeMediaQuery && typeof systemThemeMediaQuery.removeEventListener === 'function' && systemThemeListener) {
        systemThemeMediaQuery.removeEventListener('change', systemThemeListener);
        systemThemeMediaQuery = null;
        systemThemeListener = null;
    }
    // –û—á–∏—â–∞–µ–º inline background, —á—Ç–æ–±—ã —Ç–µ–º–∞ –º–æ–≥–ª–∞ –ø—Ä–∏–º–µ–Ω–∏—Ç—å—Å—è
    document.body.style.background = "";
    
    // –ü—Ä–∏–º–µ–Ω—è–µ–º —Ç–æ–ª—å–∫–æ —Ç–µ–º–Ω—É—é —Ç–µ–º—É
    if (theme === 'dark') {
        document.body.setAttribute('data-theme', 'dark');
    }
}
function saveTheme(theme) {
    localStorage.setItem('zhukbrowse_theme', theme);
}
function loadTheme() {
    const savedTheme = localStorage.getItem('zhukbrowse_theme');
    // –ï—Å–ª–∏ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∞ —Å–≤–µ—Ç–ª–∞—è –∏–ª–∏ —Å–∏—Å—Ç–µ–º–Ω–∞—è —Ç–µ–º–∞, –≤–æ–∑–≤—Ä–∞—â–∞–µ–º —Ç–µ–º–Ω—É—é
    if (savedTheme === 'light' || savedTheme === 'system') {
        return 'dark';
    }
    return savedTheme || 'dark';
}
function initThemeChooser() {
    const theme = loadTheme();
    applyTheme(theme);
    const radios = document.querySelectorAll('input[name="themeMode"]');
    radios.forEach(radio => {
        radio.checked = radio.value === theme;
        radio.onchange = () => {
            if (radio.checked) {
                applyTheme(radio.value);
                // –°–æ—Ö—Ä–∞–Ω—è–µ–º —Ç–æ–ª—å–∫–æ —Ç–µ–º–Ω—É—é —Ç–µ–º—É, –¥–ª—è —Å–≤–µ—Ç–ª–æ–π –∏ —Å–∏—Å—Ç–µ–º–Ω–æ–π –ø–æ–∫–∞–∑—ã–≤–∞–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
                if (radio.value === 'dark') {
                    saveTheme(radio.value);
                }
            }
        };
    });
}
