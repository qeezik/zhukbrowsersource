        const timeElements = document.querySelectorAll('.current-time');
        if (timeElements.length === 0) {
            return; // –ù–µ—Ç —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –≤—Ä–µ–º–µ–Ω–∏ –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
        }
        
        const now = new Date();
        const timeString = now.toLocaleTimeString('ru-RU', { 
