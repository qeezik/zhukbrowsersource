// –°–æ–∑–¥–∞–Ω–∏–µ –Ω–æ–≤–æ–π –≤–∫–ª–∞–¥–∫–∏
function createTab(url = null, title = '–ù–æ–≤–∞—è –≤–∫–ª–∞–¥–∫–∞', incognito = false) {
    if (!navTabs || !tabsContainer) {
        console.warn('–ü–æ–ø—ã—Ç–∫–∞ —Å–æ–∑–¥–∞—Ç—å –≤–∫–ª–∞–¥–∫—É –¥–æ –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏ navTabs/tabsContainer! –í–∫–ª–∞–¥–∫–∞ –Ω–µ –±—É–¥–µ—Ç —Å–æ–∑–¥–∞–Ω–∞.');
        return;
    }
    try {
        // –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –≤–∞–ª–∏–¥–Ω—ã–π url (http/https/file)
        let isValidUrl = false;
        if (url && typeof url === 'string') {
            isValidUrl = url.startsWith('http://') || url.startsWith('https://') || url.startsWith('file://');
        }
        const tabId = 'tab-' + Date.now() + '-' + Math.random().toString(36).substr(2, 9);
        // –°–æ–∑–¥–∞–µ–º –≤–∫–ª–∞–¥–∫—É –≤ –Ω–∞–≤–∏–≥–∞—Ü–∏–∏
        const tab = document.createElement('div');
        tab.className = 'tab';
        tab.setAttribute('data-tab-id', tabId);
        const tabIcon = incognito ? 'local-incognito-icon' : 'fas fa-globe';
        const tabColor = incognito ? '#b388ff' : '#fff';
        let iconHtml;
        if (incognito) {
            iconHtml = `<img src="assets/icon.svg" alt="incognito" style="width:16px;height:16px;vertical-align:middle;filter:drop-shadow(0 0 2px #b388ff);">`;
        } else {
            iconHtml = `<i class="${tabIcon}" style="color: ${tabColor};"></i>`;
        }
        tab.innerHTML = `
            ${iconHtml}
            <span style="color: ${tabColor};">${title}</span>
            <button class="tab-close" data-tab-id="${tabId}" title="–ó–∞–∫—Ä—ã—Ç—å –≤–∫–ª–∞–¥–∫—É" style="display:inline-flex;align-items:center;justify-content:center;width:22px;height:22px;margin-left:6px;background:transparent;border:none;cursor:pointer;">
                <svg width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><line x1="18" y1="6" x2="6" y2="18"></line><line x1="6" y1="6" x2="18" y2="18"></line></svg>
            </button>
        `;
        // –°–æ–∑–¥–∞–µ–º –∫–æ–Ω—Ç–µ–Ω—Ç –≤–∫–ª–∞–¥–∫–∏
        const tabContent = document.createElement('div');
        tabContent.className = 'tab-content';
        tabContent.id = tabId;
        tabContent.setAttribute('data-tab-id', tabId);
        if (isValidUrl) {
            // –¢–æ–ª—å–∫–æ webview, –µ—Å–ª–∏ –µ—Å—Ç—å –≤–∞–ª–∏–¥–Ω—ã–π url
            const webview = document.createElement('webview');
            webview.src = url;
            webview.style.width = '100%';
            webview.style.height = '100%';
            webview.style.border = 'none';
            webview.setAttribute('useragent', 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36');
            webview.setAttribute('allowpopups', '');
            webview.setAttribute('webpreferences', 'contextIsolation=no,sandbox=no,nodeIntegration=no,enableRemoteModule=no');
            webview.setAttribute('partition', incognito ? 'persist:incognito' : 'persist:main');
            
            // –î–æ–±–∞–≤–ª—è–µ–º –æ–±—Ä–∞–±–æ—Ç–∫—É –æ—à–∏–±–æ–∫ –¥–ª—è –ø—Ä–µ–¥–æ—Ç–≤—Ä–∞—â–µ–Ω–∏—è –∫—Ä–∞—à–µ–π
            webview.addEventListener('error', (e) => {
                console.error('Webview error:', e);
            });
            
            // –ü—Ä–∏–Ω—É–¥–∏—Ç–µ–ª—å–Ω–æ –ø—Ä–∏–º–µ–Ω—è–µ–º –ø—Ä–æ–∫—Å–∏ –∫ webview –µ—Å–ª–∏ –æ–Ω –≤–∫–ª—é—á–µ–Ω
            if (settings.proxyEnabled && settings.selectedCountry !== 'none') {
                const currentProxy = publicProxies.find(p => p.code === settings.selectedCountry);
                if (currentProxy) {
                    // –ü—Ä–∏–º–µ–Ω—è–µ–º –ø—Ä–æ–∫—Å–∏ —á–µ—Ä–µ–∑ main process
                    if (window.electronAPI && window.electronAPI.setProxy) {
                        window.electronAPI.setProxy({
                            host: currentProxy.host,
                            port: currentProxy.port,
                            username: currentProxy.username,
                            password: currentProxy.password,
                            code: currentProxy.code
                        });
                    }
                }
            }
            

            // ... –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ —Å–æ–±—ã—Ç–∏–π webview ...
            webview.addEventListener('dom-ready', () => {
                const theme = document.body.getAttribute('data-theme') || 'system';
                const prefers = (theme === 'dark') ? 'dark' : 'light';
                
                // –ë–µ–∑–æ–ø–∞—Å–Ω–æ–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ JavaScript —Å –æ–±—Ä–∞–±–æ—Ç–∫–æ–π –æ—à–∏–±–æ–∫
                webview.executeJavaScript(`
                    try {
                      let meta = document.querySelector('meta[name="color-scheme"]');
                      if (!meta) {
                        meta = document.createElement('meta');
                        meta.name = 'color-scheme';
                        document.head.appendChild(meta);
                      }
                      meta.content = '${prefers}';
                      if (window.matchMedia) {
                        Object.defineProperty(window, 'matchMedia', {
                          value: (q) => ({
                            matches: q.includes('dark') ? '${prefers}' === 'dark' : '${prefers}' === 'light',
                            media: q,
                            onchange: null,
                            addListener: () => {},
                            removeListener: () => {},
                            addEventListener: () => {},
                            removeEventListener: () => {},
                            dispatchEvent: () => false
                          }),
                          configurable: true
                        });
                      }
                      document.documentElement.style.colorScheme = '${prefers}';
                    } catch(e) {
                      console.warn('Theme injection error:', e);
                    }
                `).catch(error => {
                    console.warn('Failed to execute theme script:', error);
                });
            });
            webview.addEventListener('did-start-loading', () => {
                const tabSpan = tab.querySelector('span');
                if (tabSpan) {
                    const current = tabSpan.textContent.trim();
                    let isTechnical = !current || current === '–ë–µ–∑ –Ω–∞–∑–≤–∞–Ω–∏—è' || current === '–ì–æ—Ç–æ–≤–æ';
                    if (webview.src) {
                        try {
                            if (current === new URL(webview.src).hostname || current === webview.src) {
                                isTechnical = true;
                            }
                        } catch (e) {}
                    }
                    if (isTechnical) {
                        tabSpan.textContent = '–ó–∞–≥—Ä—É–∑–∫–∞...';
                    }
                }
            });
            webview.addEventListener('did-finish-load', () => {
                const tabSpan = tab.querySelector('span');
                let newTitle = webview.getTitle();
                if (!newTitle || newTitle.trim() === '') {
                    if (webview.src) {
                        try {
                            newTitle = new URL(webview.src).hostname;
                        } catch (e) {
                            newTitle = webview.src;
                        }
                    } else {
                        newTitle = '–ë–µ–∑ –Ω–∞–∑–≤–∞–Ω–∏—è';
                    }
                }
                if (tabSpan) {
                    tabSpan.textContent = newTitle;
                }
                if (!incognito && typeof addToHistory === 'function') {
                    addToHistory(url, newTitle);
                }
            });
            webview.addEventListener('page-title-updated', (event) => {
                const tabSpan = tab.querySelector('span');
                let newTitle = event.title;
                if (!newTitle || newTitle.trim() === '') {
                    if (webview.src) {
                        try {
                            newTitle = new URL(webview.src).hostname;
                        } catch (e) {
                            newTitle = webview.src;
                        }
                    } else {
                        newTitle = '–ë–µ–∑ –Ω–∞–∑–≤–∞–Ω–∏—è';
                    }
                }
                if (tabSpan) {
                    tabSpan.textContent = newTitle;
                }
            });
            webview.addEventListener('did-fail-load', (event) => {
                console.error('Webview failed to load:', event);
                const tabSpan = tab.querySelector('span');
                if (tabSpan) {
                    tabSpan.textContent = '–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏';
                }
            });
            // === –£–ª—É—á—à–µ–Ω–Ω–∞—è –æ–±—Ä–∞–±–æ—Ç–∫–∞ –æ—à–∏–±–æ–∫ webview ===
            webview.addEventListener('console-message', (e) => {
                // –§–∏–ª—å—Ç—Ä—É–µ–º —Ä–µ–∫–ª–∞–º–Ω—ã–µ –∏ —Ç—Ä–µ–∫–∏–Ω–≥–æ–≤—ã–µ —Å–æ–æ–±—â–µ–Ω–∏—è
                const isAdTrackingMessage = e.message && (
                    e.message.includes('X-Frame-Options') ||
                    e.message.includes('preloaded using link preload but not used') ||
                    e.message.includes('SilverSurfer') ||
                    e.message.includes('sync/') ||
                    e.message.includes('pixel') ||
                    e.message.includes('tracking') ||
                    e.message.includes('analytics') ||
                    e.message.includes('ads') ||
                    e.message.includes('pubmatic') ||
                    e.message.includes('ad.mrtnsvr.com') ||
                    e.message.includes('temu.com/api/adx')
                );
                
                if (isAdTrackingMessage) {
                    // –¢–∏—Ö–æ –∏–≥–Ω–æ—Ä–∏—Ä—É–µ–º —Ä–µ–∫–ª–∞–º–Ω—ã–µ —Å–æ–æ–±—â–µ–Ω–∏—è
                    return;
                }
                
                // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –≤–∞–∂–Ω—ã–µ —Å–æ–æ–±—â–µ–Ω–∏—è
                console.log('WEBVIEW LOG:', e.message);
            });
            
            // –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—à–∏–±–æ–∫ –∑–∞–≥—Ä—É–∑–∫–∏ —Å –ø–æ–ø—ã—Ç–∫–æ–π –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è
            webview.addEventListener('did-fail-load', (e) => {
                // –ü—Ä–æ–≤–µ—Ä—è–µ–º, —è–≤–ª—è–µ—Ç—Å—è –ª–∏ —ç—Ç–æ –æ—à–∏–±–∫–æ–π —Ä–µ–∫–ª–∞–º–Ω–æ–≥–æ/—Ç—Ä–µ–∫–∏–Ω–≥–æ–≤–æ–≥–æ –∫–æ–Ω—Ç–µ–Ω—Ç–∞
                const isAdTrackingError = e.errorCode === -27 || // ERR_BLOCKED_BY_RESPONSE
                                         e.errorCode === -100 || // ERR_CONNECTION_CLOSED (—á–∞—Å—Ç–æ —Ä–µ–∫–ª–∞–º–∞)
                                         e.errorCode === -320 || // ERR_INVALID_RESPONSE (—á–∞—Å—Ç–æ —Ä–µ–∫–ª–∞–º–∞)
                                         (e.validatedURL && (
                                             e.validatedURL.includes('sync/') ||
                                             e.validatedURL.includes('pixel') ||
                                             e.validatedURL.includes('tracking') ||
                                             e.validatedURL.includes('analytics') ||
                                             e.validatedURL.includes('ads') ||
                                             e.validatedURL.includes('pubmatic') ||
                                             e.validatedURL.includes('ad.mrtnsvr.com') ||
                                             e.validatedURL.includes('temu.com/api/adx')
                                         ));
                
                            if (isAdTrackingError) {
                // –¢–∏—Ö–æ –∏–≥–Ω–æ—Ä–∏—Ä—É–µ–º –æ—à–∏–±–∫–∏ —Ä–µ–∫–ª–∞–º–Ω–æ–≥–æ –∫–æ–Ω—Ç–µ–Ω—Ç–∞
                console.log('–ë–ª–æ–∫–∏—Ä–æ–≤–∞–Ω —Ä–µ–∫–ª–∞–º–Ω—ã–π/—Ç—Ä–µ–∫–∏–Ω–≥–æ–≤—ã–π –∫–æ–Ω—Ç–µ–Ω—Ç:', e.validatedURL);
                
                // –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –∑–∞–ø—Ä–æ—Å–æ–≤
                if (!window.blockedRequestsCount) {
                    window.blockedRequestsCount = 0;
                }
                window.blockedRequestsCount++;
                
                // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 10 –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –∑–∞–ø—Ä–æ—Å–æ–≤
                if (window.blockedRequestsCount % 10 === 0) {
                    showNotification(`–ó–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–æ ${window.blockedRequestsCount} —Ä–µ–∫–ª–∞–º–Ω—ã—Ö –∑–∞–ø—Ä–æ—Å–æ–≤`, 'fa-shield-alt');
                }
                
                // –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É –≤ UI
                if (typeof window.updateBlockingStats === 'function') {
                    window.updateBlockingStats();
                }
                
                return;
            }
                
                // –î–ª—è –æ—Å—Ç–∞–ª—å–Ω—ã—Ö –æ—à–∏–±–æ–∫ –ø–æ–∫–∞–∑—ã–≤–∞–µ–º –≤ –∫–æ–Ω—Å–æ–ª–∏
                console.error('WEBVIEW FAIL:', e.errorDescription, e.validatedURL);
                const tabSpan = tab.querySelector('span');
                if (tabSpan) {
                    tabSpan.textContent = '–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏';
                }
                
                // –ü–æ–ø—ã—Ç–∫–∞ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è —Ç–æ–ª—å–∫–æ –¥–ª—è –∫—Ä–∏—Ç–∏—á–µ—Å–∫–∏—Ö –æ—à–∏–±–æ–∫
                if (e.errorCode === -3 || e.errorCode === -6) {
                    console.log('–ü–æ–ø—ã—Ç–∫–∞ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è webview...');
                    setTimeout(() => {
                        try {
                            webview.reload();
                        } catch (reloadError) {
                            console.error('–û—à–∏–±–∫–∞ –ø—Ä–∏ –ø–æ–ø—ã—Ç–∫–µ –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∏:', reloadError);
                        }
                    }, 2000);
                }
            });
            
            // –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫—Ä–∞—à–∞ –ø—Ä–æ—Ü–µ—Å—Å–∞ —Ä–µ–Ω–¥–µ—Ä–∏–Ω–≥–∞
            webview.addEventListener('render-process-gone', (event) => {
                console.error('WEBVIEW CRASHED:', event.reason);
                const tabSpan = tab.querySelector('span');
                if (tabSpan) {
                    tabSpan.textContent = '–ö—Ä–∞—Ö –ø—Ä–æ—Ü–µ—Å—Å–∞';
                }
                
                // –ü–æ–ø—ã—Ç–∫–∞ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è –ø–æ—Å–ª–µ –∫—Ä–∞—à–∞
                setTimeout(() => {
                    try {
                        webview.reload();
                    } catch (reloadError) {
                        console.error('–û—à–∏–±–∫–∞ –ø—Ä–∏ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–∏ –ø–æ—Å–ª–µ –∫—Ä–∞—à–∞:', reloadError);
                    }
                }, 3000);
            });
            
            // –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—à–∏–±–æ–∫ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è —Å–∫—Ä–∏–ø—Ç–æ–≤
            webview.addEventListener('did-fail-provisional-load', (e) => {
                console.error('WEBVIEW PROVISIONAL FAIL:', e.errorDescription, e.validatedURL);
            });
            
            // –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—à–∏–±–æ–∫ SSL
            webview.addEventListener('certificate-error', (e) => {
                console.error('WEBVIEW SSL ERROR:', e.errorDescription);
                // –î–ª—è SSL –æ—à–∏–±–æ–∫ –º–æ–∂–Ω–æ –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å –∑–∞–≥—Ä—É–∑–∫—É
                e.preventDefault();
                e.stopPropagation();
            });
            
            // –§—É–Ω–∫—Ü–∏—è –±–µ–∑–æ–ø–∞—Å–Ω–æ–π –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∏ webview
            const safeReload = () => {
                try {
                    if (webview && webview.src) {
                        console.log('–ë–µ–∑–æ–ø–∞—Å–Ω–∞—è –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞ webview...');
                        webview.reload();
                    }
                } catch (error) {
                    console.error('–û—à–∏–±–∫–∞ –ø—Ä–∏ –±–µ–∑–æ–ø–∞—Å–Ω–æ–π –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–µ:', error);
                }
            };
            
            // –ì–ª–æ–±–∞–ª—å–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∏ webview –∏–∑ –¥—Ä—É–≥–∏—Ö –º–µ—Å—Ç
            window.safeReloadWebview = safeReload;
            
            // –ì–ª–æ–±–∞–ª—å–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –±–ª–æ–∫–∏—Ä–æ–≤–∫–∏
            window.getBlockedRequestsStats = () => {
                return {
                    count: window.blockedRequestsCount || 0,
                    lastUpdate: new Date().toLocaleTimeString()
                };
            };
            
            // –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏ –±–ª–æ–∫–∏—Ä–æ–≤–∫–∏ –≤ UI
            window.updateBlockingStats = () => {
                const stats = window.getBlockedRequestsStats();
                const blockedCountEl = document.getElementById('blockedCount');
                const lastUpdateEl = document.getElementById('lastUpdate');
                
                if (blockedCountEl) {
                    blockedCountEl.textContent = stats.count;
                }
                if (lastUpdateEl) {
                    lastUpdateEl.textContent = stats.lastUpdate;
                }
            };
            // === –î–û–ë–ê–í–õ–ï–ù–û: –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è –∫–Ω–æ–ø–æ–∫ –Ω–∞–≤–∏–≥–∞—Ü–∏–∏ –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ –∏—Å—Ç–æ—Ä–∏–∏ ===
            webview.addEventListener('did-navigate', updateNavButtons);
            webview.addEventListener('did-navigate-in-page', updateNavButtons);
            webview.addEventListener('did-frame-navigate', updateNavButtons);
            webview.addEventListener('did-start-navigation', updateNavButtons);
            webview.addEventListener('did-stop-loading', updateNavButtons);
            tabContent.appendChild(webview);
        } else {
            // –¢–æ–ª—å–∫–æ —Å—Ç–∞—Ä—Ç–æ–≤–∞—è —Å—Ç—Ä–∞–Ω–∏—Ü–∞, –µ—Å–ª–∏ url –Ω–µ–≤–∞–ª–∏–¥–Ω—ã–π –∏–ª–∏ –ø—É—Å—Ç–æ–π
            const startPageDiv = document.createElement('div');
            startPageDiv.className = 'start-page' + (incognito ? ' incognito' : '');
            startPageDiv.id = 'startPage-' + tabId;
            startPageDiv.style.position = 'relative';
            startPageDiv.style.width = '100%';
            startPageDiv.style.height = '100%';
            tabContent.appendChild(startPageDiv);
        }
        if (navTabs) navTabs.appendChild(tab);
        if (tabsContainer) tabsContainer.appendChild(tabContent);
        
        // –°–∫—Ä—ã–≤–∞–µ–º –≤—Å–µ –¥—Ä—É–≥–∏–µ –≤–∫–ª–∞–¥–∫–∏ –ø–µ—Ä–µ–¥ –ø–æ–∫–∞–∑–æ–º –Ω–æ–≤–æ–π
        const allTabContents = document.querySelectorAll('.tab-content');
        allTabContents.forEach(content => {
            if (content.id !== tabId) {
                content.style.display = 'none';
                content.classList.remove('active');
            }
        });
        
        // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –Ω–æ–≤—É—é –≤–∫–ª–∞–¥–∫—É
        tabContent.style.display = 'block';
        tabContent.classList.add('active');
        
        if (!isValidUrl) {
            const renderStartPageWithDelay = () => {
                if (typeof renderStartPage === 'function') {
                    renderStartPage('startPage-' + tabId, incognito ? '#b388ff' : '#00ff88');
                    if (typeof initStartPage === 'function') {
                        initStartPage('startPage-' + tabId);
                    }
                }
            };
            if (document.readyState === 'complete') {
                renderStartPageWithDelay();
            } else {
                setTimeout(renderStartPageWithDelay, 50);
            }
        }
        addTabEventListeners(tab);
        switchTab(tabId);
        updateNavButtons();
        return tabId;
    } catch (error) {
        console.error('Error creating tab:', error);
        return null;
    }
}

function switchTab(tabId) {
    try {
        if (!tabId) {
            console.error('No tabId provided to switchTab');
            return;
        }
        
        // –£–±–∏—Ä–∞–µ–º –∞–∫—Ç–∏–≤–Ω—ã–π –∫–ª–∞—Å—Å —Å–æ –≤—Å–µ—Ö –≤–∫–ª–∞–¥–æ–∫
        const allTabs = document.querySelectorAll('.tab');
        const allTabContents = document.querySelectorAll('.tab-content');
        
        allTabs.forEach(tab => tab.classList.remove('active'));
        allTabContents.forEach(content => {
            content.classList.remove('active');
            // –°–∫—Ä—ã–≤–∞–µ–º –≤—Å–µ –∫–æ–Ω—Ç–µ–Ω—Ç—ã –≤–∫–ª–∞–¥–æ–∫
            content.style.display = 'none';
        });
        
        // –î–æ–±–∞–≤–ª—è–µ–º –∞–∫—Ç–∏–≤–Ω—ã–π –∫–ª–∞—Å—Å –∫ –≤—ã–±—Ä–∞–Ω–Ω–æ–π –≤–∫–ª–∞–¥–∫–µ
        const selectedTab = document.querySelector(`[data-tab-id="${tabId}"]`);
        const selectedTabContent = document.getElementById(tabId);
        
        if (selectedTab) {
            selectedTab.classList.add('active');
        } else {
            console.warn(`Tab with id '${tabId}' not found in navigation`);
        }
        
        if (selectedTabContent) {
            selectedTabContent.classList.add('active');
            selectedTabContent.style.display = 'block';
            console.log(`Tab content ${tabId} marked as active`);
        } else {
            console.warn(`Tab content with id '${tabId}' not found`);
        }
        
        // –û–±–Ω–æ–≤–ª—è–µ–º –∞–∫—Ç–∏–≤–Ω—É—é –≤–∫–ª–∞–¥–∫—É
        activeTabId = tabId;
        
        // –û–±–Ω–æ–≤–ª—è–µ–º –∞–¥—Ä–µ—Å–Ω—É—é —Å—Ç—Ä–æ–∫—É
        if (urlInput) {
            const webview = selectedTabContent ? selectedTabContent.querySelector('webview') : null;
            if (webview) {
                urlInput.value = webview.src;
            } else {
                urlInput.value = '';
            }
        }
        
        // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º/—Å–∫—Ä—ã–≤–∞–µ–º –∫–Ω–æ–ø–∫—É –≥–∞–ª–µ—Ä–µ–∏ —Ñ–æ–Ω–æ–≤
        const galleryBtn = document.getElementById('backgroundGalleryBtn');
        if (galleryBtn) {
            // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –∫–Ω–æ–ø–∫—É –Ω–∞ —Å—Ç–∞—Ä—Ç–æ–≤–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü–µ –∏ –Ω–æ–≤—ã—Ö –≤–∫–ª–∞–¥–∫–∞—Ö (–±–µ–∑ webview)
            const selectedTabContent = document.getElementById(tabId);
            const hasWebview = selectedTabContent && selectedTabContent.querySelector('webview');
            if (tabId === 'start' || !hasWebview) {
                galleryBtn.style.display = 'block';
            } else {
                galleryBtn.style.display = 'none';
            }
        }
        
        // –û–±–Ω–æ–≤–ª—è–µ–º –∫–Ω–æ–ø–∫–∏ –Ω–∞–≤–∏–≥–∞—Ü–∏–∏
        updateNavButtons();
        
        console.log(`Switched to tab: ${tabId}`);
        
    } catch (error) {
        console.error('Error switching tab:', error);
    }
}

// –ó–∞–∫—Ä—ã—Ç–∏–µ –≤–∫–ª–∞–¥–∫–∏
function closeTab(tabId) {
    try {
        if (!tabId || tabId === 'start') {
            console.log('Cannot close start tab');
            return;
        }
        
        const tab = document.querySelector(`[data-tab-id="${tabId}"]`);
        const tabContent = document.getElementById(tabId);
        
        if (tab) {
            tab.remove();
        } else {
            console.warn(`Tab with id '${tabId}' not found in navigation`);
        }
        
        if (tabContent) {
            tabContent.remove();
        } else {
            console.warn(`Tab content with id '${tabId}' not found`);
        }
        
        // –ï—Å–ª–∏ –∑–∞–∫—Ä—ã–ª–∏ –∞–∫—Ç–∏–≤–Ω—É—é –≤–∫–ª–∞–¥–∫—É, –ø–µ—Ä–µ–∫–ª—é—á–∞–µ–º—Å—è –Ω–∞ —Å—Ç–∞—Ä—Ç–æ–≤—É—é
        if (activeTabId === tabId) {
            switchTab('start');
        }
        
        console.log(`Tab closed: ${tabId}`);
        
    } catch (error) {
        console.error('Error closing tab:', error);
    }
}

// –ù–∞–≤–∏–≥–∞—Ü–∏—è –∫ URL
function navigateTo(url) {
    try {
        if (!url) {
            console.error('No URL provided to navigateTo');
            return;
        }
        
        // –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–∞—Ö–æ–¥–∏–º—Å—è –ª–∏ –º—ã –≤ —Ä–µ–∂–∏–º–µ –∏–Ω–∫–æ–≥–Ω–∏—Ç–æ
        const activeTabContent = document.getElementById(activeTabId);
        const isIncognito = activeTabContent && activeTabContent.getAttribute('data-incognito') === '1';
        
        // –ü—Ä–æ–≤–µ—Ä—è–µ–º, —è–≤–ª—è–µ—Ç—Å—è –ª–∏ —ç—Ç–æ –ø–æ–∏—Å–∫–æ–≤—ã–º –∑–∞–ø—Ä–æ—Å–æ–º
        if (!url.startsWith('http://') && !url.startsWith('https://') && !url.startsWith('file://')) {
            // –≠—Ç–æ –ø–æ–∏—Å–∫–æ–≤—ã–π –∑–∞–ø—Ä–æ—Å
            if (isIncognito) {
                // –í —Ä–µ–∂–∏–º–µ –∏–Ω–∫–æ–≥–Ω–∏—Ç–æ –∏—Å–ø–æ–ª—å–∑—É–µ–º webview –¥–ª—è –ø–æ–∏—Å–∫–∞
                navigateInIncognito(url);
            } else if (typeof performSearch === 'function') {
                performSearch(url);
            } else {
                console.error('performSearch function not available');
            }
            return;
        }
        
        // –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—É—é –≤–∫–ª–∞–¥–∫—É —Å URL
        if (typeof createTab === 'function') {
            createTab(url);
        } else {
            console.error('createTab function not available');
        }
        
    } catch (error) {
        console.error('Error navigating to URL:', error);
    }
}

// –°–ø–µ—Ü–∏–∞–ª—å–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –Ω–∞–≤–∏–≥–∞—Ü–∏–∏ –≤ —Ä–µ–∂–∏–º–µ –∏–Ω–∫–æ–≥–Ω–∏—Ç–æ
function navigateInIncognito(url) {
    try {
        const activeTabContent = document.getElementById(activeTabId);
        if (!activeTabContent) return;
        
        const startPage = activeTabContent.querySelector('#startPage');
        const webview = activeTabContent.querySelector('#incognitoWebview');
        
        if (!startPage || !webview) return;
        
        // –°–∫—Ä—ã–≤–∞–µ–º —Å—Ç–∞—Ä—Ç–æ–≤—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É –∏ –ø–æ–∫–∞–∑—ã–≤–∞–µ–º webview
        startPage.style.display = 'none';
        webview.style.display = 'block';
        
        // –§–æ—Ä–º–∏—Ä—É–µ–º URL –¥–ª—è –ø–æ–∏—Å–∫–∞
        let searchUrl;
        if (settings.searchEngine === 'google') {
            searchUrl = `https://www.google.com/search?q=${encodeURIComponent(url)}`;
        } else if (settings.searchEngine === 'bing') {
            searchUrl = `https://www.bing.com/search?q=${encodeURIComponent(url)}`;
        } else {
            searchUrl = `https://duckduckgo.com/?q=${encodeURIComponent(url)}`;
        }
        
        // –ó–∞–≥—Ä—É–∂–∞–µ–º URL –≤ webview
        webview.src = searchUrl;
        
        // –û–±–Ω–æ–≤–ª—è–µ–º URL –≤ –∞–¥—Ä–µ—Å–Ω–æ–π —Å—Ç—Ä–æ–∫–µ
        if (urlInput) {
            urlInput.value = searchUrl;
        }
        
        // –ü—Ä–∏–º–µ–Ω—è–µ–º –∑–∞—â–∏—Ç—É –∫ webview
        if (typeof applyIncognitoProtectionToWebview === 'function') {
            applyIncognitoProtectionToWebview(webview);
        }
        
        showNotification('–ü–æ–∏—Å–∫ –≤—ã–ø–æ–ª–Ω–µ–Ω –≤ —Ä–µ–∂–∏–º–µ –∏–Ω–∫–æ–≥–Ω–∏—Ç–æ');
        
    } catch (error) {
        console.error('Error navigating in incognito:', error);
    }
}

// –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–Ω–æ–ø–æ–∫ –Ω–∞–≤–∏–≥–∞—Ü–∏–∏
function updateNavButtons() {
    try {
        const activeTabContent = document.getElementById(activeTabId);
        if (!activeTabContent) {
            console.warn('Active tab content not found');
            return;
        }
        const webview = activeTabContent.querySelector('webview');
        const isIncognito = activeTabContent.getAttribute('data-incognito') === '1';
        // –ü—Ä–æ–≤–µ—Ä–∫–∞: webview –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –≤ DOM –∏ –≥–æ—Ç–æ–≤
        let canGoBack = false, canGoForward = false;
        if (webview && webview.isConnected && webview.getWebContentsId) {
            try {
                canGoBack = typeof webview.canGoBack === 'function' && webview.canGoBack();
                canGoForward = typeof webview.canGoForward === 'function' && webview.canGoForward();
            } catch (e) {
                // webview –Ω–µ –≥–æ—Ç–æ–≤, –Ω–µ –æ–±–Ω–æ–≤–ª—è–µ–º
                canGoBack = false;
                canGoForward = false;
            }
        }
        // –û–±–Ω–æ–≤–ª—è–µ–º –∫–Ω–æ–ø–∫–∏ –Ω–∞–∑–∞–¥/–≤–ø–µ—Ä–µ–¥
        if (backBtn) {
            if (isIncognito) {
                backBtn.disabled = false;
            } else {
                backBtn.disabled = !canGoBack;
            }
        }
        if (forwardBtn) {
            if (isIncognito) {
                forwardBtn.disabled = false;
