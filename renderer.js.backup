// –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–µ–º–∏—É–º —Å—Ç–∞—Ç—É—Å–∞
function isPremiumUser() {
    const saved = localStorage.getItem('zhukbrowse_profile');
    if (saved) {
        const profileData = JSON.parse(saved);
        
        // –ü—Ä–æ–≤–µ—Ä—è–µ–º, –µ—Å—Ç—å –ª–∏ –∞–∫—Ç–∏–≤–Ω–∞—è –ø–æ–¥–ø–∏—Å–∫–∞
        if (profileData.isPremium === true) {
            // –ï—Å–ª–∏ —ç—Ç–æ –ø–æ–∂–∏–∑–Ω–µ–Ω–Ω–∞—è –ø–æ–¥–ø–∏—Å–∫–∞, –≤—Å–µ–≥–¥–∞ –≤–æ–∑–≤—Ä–∞—â–∞–µ–º true
            if (profileData.isLifetime === true) {
                console.log('isPremiumUser check: lifetime subscription, returning true');
                return true;
            }
            
            // –î–ª—è –æ–±—ã—á–Ω—ã—Ö –ø–æ–¥–ø–∏—Å–æ–∫ –ø—Ä–æ–≤–µ—Ä—è–µ–º —Å—Ä–æ–∫ –¥–µ–π—Å—Ç–≤–∏—è
            if (profileData.expiresAt) {
                const now = new Date();
                const expiresAt = new Date(profileData.expiresAt);
                
                if (now < expiresAt) {
                    console.log('isPremiumUser check: subscription active, returning true');
                    return true;
                } else {
                    // –ü–æ–¥–ø–∏—Å–∫–∞ –∏—Å—Ç–µ–∫–ª–∞, —Å–±—Ä–∞—Å—ã–≤–∞–µ–º —Å—Ç–∞—Ç—É—Å
                    console.log('isPremiumUser check: subscription expired, resetting status');
                    profileData.isPremium = false;
                    profileData.expiresAt = null;
                    saveProfileData(profileData);
                    return false;
                }
            } else {
                // –ï—Å–ª–∏ –Ω–µ—Ç –¥–∞—Ç—ã –∏—Å—Ç–µ—á–µ–Ω–∏—è, —Å—á–∏—Ç–∞–µ–º –ø–æ–¥–ø–∏—Å–∫—É –∞–∫—Ç–∏–≤–Ω–æ–π
                console.log('isPremiumUser check: no expiration date, returning true');
                return true;
            }
        }
        
        console.log('isPremiumUser check: no premium subscription, returning false');
        return false;
    }
    console.log('isPremiumUser check: no profile data, returning false');
    return false;
}

