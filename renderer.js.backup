    const closeBtn = menu.querySelector('.close-menu-btn');
    closeBtn.addEventListener('click', () => {
        menu.remove();
    });
    
    // –í—ã–±–æ—Ä —Å—Ç—Ä–∞–Ω—ã
    menu.querySelectorAll('.proxy-country').forEach(countryEl => {
        countryEl.addEventListener('click', () => {
            const countryCode = countryEl.getAttribute('data-country');
            const country = proxyServers[countryCode];
            
            settings.proxyEnabled = true;
            settings.selectedCountry = countryCode;
            settings.proxyHost = country.host;
            settings.proxyPort = country.port;
            
            updateProxyButton();
            localStorage.setItem('zhukbrowse_settings', JSON.stringify(settings));
            showNotification(`VPN –ø–æ–¥–∫–ª—é—á–µ–Ω –∫ ${country.flag} ${country.name}`);
            
            menu.remove();
            
            // –ü–µ—Ä–µ–∑–∞–≥—Ä—É–∂–∞–µ–º –≤—Å–µ –∞–∫—Ç–∏–≤–Ω—ã–µ webview —Å –Ω–æ–≤—ã–º –ø—Ä–æ–∫—Å–∏
            reloadAllWebviews();
        });
    });
    
    // –ó–∞–∫—Ä—ã—Ç–∏–µ –ø–æ –∫–ª–∏–∫—É –≤–Ω–µ –º–µ–Ω—é
    document.addEventListener('click', function closeMenu(e) {
        if (!menu.contains(e.target) && e.target !== proxyToggle) {
            menu.remove();
            document.removeEventListener('click', closeMenu);
        }
    });
}

// –ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞ –≤—Å–µ—Ö webview
function reloadAllWebviews() {
    console.log('–ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞ –≤—Å–µ—Ö webview...');
    document.querySelectorAll('webview').forEach((webview, index) => {
        try {
            const currentUrl = webview.src;
            if (currentUrl && currentUrl !== 'about:blank') {
                console.log(`–ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞ webview ${index + 1}: ${currentUrl}`);
                webview.reload();
            }
        } catch (error) {
            console.error(`–û—à–∏–±–∫–∞ –ø—Ä–∏ –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–µ webview ${index + 1}:`, error);
            
            // –ü–æ–ø—ã—Ç–∫–∞ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è webview
            try {
                if (webview && webview.src) {
                    setTimeout(() => {
                        try {
                            webview.reload();
                        } catch (reloadError) {
                            console.error('–ü–æ–≤—Ç–æ—Ä–Ω–∞—è –æ—à–∏–±–∫–∞ –ø—Ä–∏ –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–µ:', reloadError);
                        }
                    }, 1000);
                }
            } catch (recoveryError) {
                console.error('–û—à–∏–±–∫–∞ –ø—Ä–∏ –ø–æ–ø—ã—Ç–∫–µ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è:', recoveryError);
            }
        }
    });
}

