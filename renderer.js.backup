// –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–æ–≤ —Å–æ–±—ã—Ç–∏–π –¥–ª—è –≤–∫–ª–∞–¥–∫–∏
function addTabEventListeners(tab) {
    const tabId = tab.getAttribute('data-tab-id');
    if (!tabId) {
        console.log('No tab ID found for tab:', tab);
        return;
    }
    
    console.log('Adding event listeners for tab:', tabId);
    
