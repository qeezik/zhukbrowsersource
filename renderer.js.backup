// ===== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –ë–ï–ó–û–ü–ê–°–ù–û–°–¢–¨ –ò–ù–ö–û–ì–ù–ò–¢–û =====

// –ö–ª—é—á —à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—è (–≤ —Ä–µ–∞–ª—å–Ω–æ–º –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–∏ –¥–æ–ª–∂–µ–Ω –±—ã—Ç—å –±–æ–ª–µ–µ —Å–ª–æ–∂–Ω—ã–º)
const ENCRYPTION_KEY = 'zhukbrowse_secure_key_2024';

// –§—É–Ω–∫—Ü–∏—è —à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö
// –†–∞—Å—à–∏—Ä–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—è —Å AES
function encryptData(data) {
    try {
        // –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º –∫–ª—é—á –Ω–∞ –æ—Å–Ω–æ–≤–µ —É–Ω–∏–∫–∞–ª—å–Ω–æ–≥–æ –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä–∞ –±—Ä–∞—É–∑–µ—Ä–∞
        const browserId = getBrowserId();
        const key = generateKeyFromString(browserId);
        
        // –ò—Å–ø–æ–ª—å–∑—É–µ–º AES –¥–ª—è —à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—è
        const text = JSON.stringify(data);
        const encrypted = CryptoJS.AES.encrypt(text, key).toString();
        return encrypted;
    } catch (error) {
        console.error('Encryption error:', error);
        // Fallback –∫ –ø—Ä–æ—Å—Ç–æ–º—É —à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—é
        try {
            const text = JSON.stringify(data);
            return btoa(encodeURIComponent(text));
        } catch (fallbackError) {
            console.error('Fallback encryption failed:', fallbackError);
            return null;
        }
    }
}

// –†–∞—Å—à–∏—Ä–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–µ—à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—è
function decryptData(encryptedData) {
    try {
        // –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º –∫–ª—é—á –Ω–∞ –æ—Å–Ω–æ–≤–µ —É–Ω–∏–∫–∞–ª—å–Ω–æ–≥–æ –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä–∞ –±—Ä–∞—É–∑–µ—Ä–∞
        const browserId = getBrowserId();
        const key = generateKeyFromString(browserId);
        
        // –†–∞—Å—à–∏—Ñ—Ä–æ–≤—ã–≤–∞–µ–º AES
        const decrypted = CryptoJS.AES.decrypt(encryptedData, key).toString(CryptoJS.enc.Utf8);
        return JSON.parse(decrypted);
    } catch (error) {
        console.error('Decryption error:', error);
        // Fallback –∫ –ø—Ä–æ—Å—Ç–æ–º—É –¥–µ—à–∏—Ñ—Ä–æ–≤–∞–Ω–∏—é
        try {
            const decoded = decodeURIComponent(atob(encryptedData));
            return JSON.parse(decoded);
        } catch (fallbackError) {
            console.error('Fallback decryption failed:', fallbackError);
            return null;
        }
    }
}

// –ì–µ–Ω–µ—Ä–∞—Ü–∏—è —É–Ω–∏–∫–∞–ª—å–Ω–æ–≥–æ –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä–∞ –±—Ä–∞—É–∑–µ—Ä–∞
function getBrowserId() {
    let browserId = localStorage.getItem('zhukbrowse_browser_id');
    if (!browserId) {
        // –°–æ–∑–¥–∞–µ–º —É–Ω–∏–∫–∞–ª—å–Ω—ã–π ID –Ω–∞ –æ—Å–Ω–æ–≤–µ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫ –±—Ä–∞—É–∑–µ—Ä–∞
        const canvas = document.createElement('canvas');
        const ctx = canvas.getContext('2d');
        ctx.textBaseline = 'top';
        ctx.font = '14px Arial';
        ctx.fillText('ZhukBrowse', 2, 2);
        
        const fingerprint = [
            navigator.userAgent,
            navigator.language,
            screen.width + 'x' + screen.height,
            new Date().getTimezoneOffset(),
            ctx.canvas.toDataURL()
        ].join('|');
        
        browserId = btoa(fingerprint).substring(0, 32);
        localStorage.setItem('zhukbrowse_browser_id', browserId);
    }
    return browserId;
}

// –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –∫–ª—é—á–∞ –∏–∑ —Å—Ç—Ä–æ–∫–∏
function generateKeyFromString(str) {
    // –°–æ–∑–¥–∞–µ–º —Ö–µ—à –∏–∑ —Å—Ç—Ä–æ–∫–∏ –¥–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –∫–ª—é—á–∞
    let hash = 0;
    for (let i = 0; i < str.length; i++) {
        const char = str.charCodeAt(i);
        hash = ((hash << 5) - hash) + char;
        hash = hash & hash; // Convert to 32bit integer
    }
    
    // –ü—Ä–µ–æ–±—Ä–∞–∑—É–µ–º —Ö–µ—à –≤ —Å—Ç—Ä–æ–∫—É –∫–ª—é—á–∞
    const keyString = Math.abs(hash).toString(16).padStart(8, '0');
    return 'zhukbrowse_' + keyString + '_secure_key_2024';
}

